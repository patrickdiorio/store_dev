{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "adf-store-dev"
		},
		"AzureDataLakeStorage1_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'AzureDataLakeStorage1'"
		},
		"AzureDataLakeStorage1_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://adlsspacex.dfs.core.windows.net/"
		},
		"GabrielSena_properties_typeProperties_baseUrl": {
			"type": "string",
			"defaultValue": "https://kvstoredev.vault.azure.net/"
		},
		"ls_databricks_properties_typeProperties_existingClusterId": {
			"type": "string",
			"defaultValue": "0709-123013-3nxxijia"
		},
		"ls_datalake_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://adlsstoredev.dfs.core.windows.net/"
		},
		"trigger_store_pedidos_properties_typeProperties_scope": {
			"type": "string",
			"defaultValue": "/subscriptions/d23e7f59-09d7-417d-a6a8-78eb59751d65/resourceGroups/rg_store_dev/providers/Microsoft.Storage/storageAccounts/adlsstoredev"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "adf-store-dev_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"AzureDataLakeStorage1_accountKey": {
						"value": "[parameters('AzureDataLakeStorage1_accountKey')]"
					},
					"AzureDataLakeStorage1_properties_typeProperties_url": {
						"value": "[parameters('AzureDataLakeStorage1_properties_typeProperties_url')]"
					},
					"GabrielSena_properties_typeProperties_baseUrl": {
						"value": "[parameters('GabrielSena_properties_typeProperties_baseUrl')]"
					},
					"ls_databricks_properties_typeProperties_existingClusterId": {
						"value": "[parameters('ls_databricks_properties_typeProperties_existingClusterId')]"
					},
					"ls_datalake_properties_typeProperties_url": {
						"value": "[parameters('ls_datalake_properties_typeProperties_url')]"
					},
					"trigger_store_pedidos_properties_typeProperties_scope": {
						"value": "[parameters('trigger_store_pedidos_properties_typeProperties_scope')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}